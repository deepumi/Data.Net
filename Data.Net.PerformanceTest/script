CREATE TABLE teams
   (
      id NUMBER(7) NOT NULL,
      
      firstname VARCHAR2(70) NOT NULL,
      lastname VARCHAR2(70) NOT NULL
   );
 
   CREATE SEQUENCE teams_seq START WITH 1;
 
   CREATE OR REPLACE TRIGGER teams_id_tr BEFORE INSERT
     ON teams
   FOR EACH ROW
   BEGIN
      SELECT teams_seq.nextval INTO :new.id FROM dual;
   END;
   /
   
   DROP SEQUENCE {SCHEMA_NAME}.TEAMS_SEQ;
   
   DROP TABLE teams